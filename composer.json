{
  "name": "fgh151/yii2-swagger",
  "type": "yii2-extension",
  "minimum-stability": "dev",
  "version": "0.0.4",
  "description": "yii2 swagger files generator",
  "keywords": ["yii2","swagger"],
  "homepage": "https://openitstudio.ru",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Fedor B Gorsky",
      "email": "fedor@support-pc.org"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "yiisoft/yii2": "^2.0.45",
    "zircote/swagger-php": "^5.0",
    "doctrine/annotations": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "fgh151\\swagger\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
